{
  "name": "express-drizzle-better-auth-starter",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "node --watch ./src/index.js",
    "test": "jest",
    "db:generate": "npx drizzle-kit generate --dialect=sqlite --schema=./src/db/schema/schema.js",
    "db:migrate": "npx drizzle-kit migrate --dialect=sqlite",
    "db:push": "dotenv -- cross-var npx drizzle-kit push --schema=./src/db/schema/schema.js --dialect=sqlite --url=%DATABASE_URL%",
    "db:studio": "npx drizzle-kit studio",
    "seed": "node ./src/db/seed.js",
    "auth:schema": "npx @better-auth/cli@latest generate --output ./src/db/schema/auth-schema.js --config ./src/utils/auth.js",
    "postman": "express-to-postman -i ./src/index.js -o ./postman.collection.json",
    "swagger": "node ./swagger.js",
    "auth:seed": "npx @better-auth-kit/cli@latest seed",
    "auth:studio": "npx @better-auth-kit/cli db"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "description": "",
  "dependencies": {
    "@better-auth-kit/seed": "^1.0.12",
    "@libsql/client": "^0.15.10",
    "better-auth": "^1.3.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-var": "^1.1.0",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.4",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "nodemailer": "^7.0.5",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "dotenv-cli": "^10.0.0",
    "drizzle-kit": "^0.31.4",
    "express-to-postman": "^1.1.3",
    "jest": "^30.0.5",
    "supertest": "^7.1.4",
    "swagger-autogen": "^2.23.7"
  }
}
